   "source": [
    "def last(n):\n",
    "    return n[-1]  \n",
    "   \n",
    "def sort(tuples):\n",
    "    return sorted(tuples, key=last)\n",
    "   \n",
    "a=[(1, 3), (3, 2), (2, 1)]\n",
    "print(\"Sorted:\")\n",
    "print(sort(a))"
   ]
